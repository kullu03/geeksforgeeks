struct node * inOrderSuccessor(struct node *root, struct node *n,struct node **ans)//saves inorder successor in ans
{
	static int bit=0;
  if(root == NULL)
	 return NULL;
  inOrderSuccessor(root-&gt;left,n,ans);

  if(bit)
  {*ans=root;bit=0;}
  if(n==root)
	  bit=1;
  inOrderSuccessor(root-&gt;right,n,ans);
  
}
</code> </pre>
