     if(root)<br />
     {<br />
             inorder(root-&gt;left);<br />
             arr[len++]=root-&gt;data;<br />
             inorder(root-&gt;right);<br />
     }<br />
}<br />
void find(struct node *a)<br />
{<br />
     int i;<br />
     for(i=0;idata)<br />
                       printf(&#8220;%d&#8221;,arr[i+1]);<br />
     }<br />
