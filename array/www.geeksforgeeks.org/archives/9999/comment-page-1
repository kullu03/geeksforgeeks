<pre> <code language="C">
int inorderPredecessor(struct node *root, struct node *node)
{
	struct node *pred = NULL;

	if(node-&gt;rchild != NULL)
		return minOf(node-&gt;rchild);	
	
	while(root)
	{
		if(node-&gt;data &lt; root-&gt;data)
		{
			pred = root;
			root = root-&gt;lchild;
		}
		else
		{
			if(node-&gt;data &gt; root-&gt;data)
			{				
				root = root-&gt;rchild;

				if(node-&gt;data &lt; root-&gt;data)
					pred = root;
			}
			else
				break;
		}
	}

	if(pred!=NULL)
		return pred-&gt;data;
	else
		return -1;
}
</code> </pre>
