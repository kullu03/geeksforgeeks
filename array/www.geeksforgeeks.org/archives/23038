<p>/********* Interview Round 1 ******************/</p>
<p>//Question 1<br />
Node getNewLinkedList(Node head, int K) {<br />
	Node firstNode = head;<br />
	Node secondNode = head;<br />
	int i = 1;<br />
	while(i &lt; K &amp;&amp; secondNode != null) {<br />
	  secondNode = secondNode.next;<br />
	}</p>
<p>	while(secondNode != null) {<br />
	 int temp = secondNode.data;<br />
	 secondNode.data = firstNode.data;<br />
	 firstNode.data = temp;</p>
<p>	 int i = 1;<br />
	 while(i &lt;= K &amp;&amp; secondNode != null) {<br />
	   secondNode = secondNode.next;<br />
	   firstNode = firstNode.next;<br />
	}<br />
	}</p>
<p>	return head;<br />
}</p>
<p>//Question 2<br />
Node getNewLinkedList(Node head, int K) {<br />
	Node firstNode = null;<br />
	Node secondNode = head;<br />
	int i = 1;<br />
	while(i &lt; K-1 &amp;&amp; secondNode != null) {<br />
	  secondNode = secondNode.next;<br />
	}</p>
<p>	while(secondNode != null) {<br />
	 if(firstNode == null) {<br />
	  Node temp = secondNode.next;<br />
	  secondNode.next = head;<br />
	  head = temp;<br />
	  firstNode = head;<br />
	  secondNode = secondNode.next;<br />
	  }<br />
	  else {<br />
	   Node temp = secondNode.next;<br />
	   secondNode.next = firstNode.next;<br />
	   firstNode.next = temp;<br />
	   firstNode = firstNode.next;<br />
	   secondNode = secondNode.next;<br />
	}</p>
<p>	 int i = 1;<br />
	 while(i &lt; K &amp;&amp; secondNode != null) {<br />
	   secondNode = secondNode.next;<br />
	   firstNode = firstNode.next;<br />
	}<br />
	}</p>
<p>	return head;<br />
}</p>
<p>/********* Interview Round 2 ******************/</p>
<p>//Question 1-3 KDimension</p>
<p>int getMaxSlab(Slab[] slabs) {<br />
  int[] count = new int[slabs.length];<br />
  max[0] = 1;<br />
  sortFirstDim(slabs);</p>
<p>  for(int i = 0; i &lt; slabs.length; ++i) {<br />
   int max = -1;<br />
   for(int j = 0; j &lt; i; ++j) {<br />
    boolean res = true;<br />
    for(int k = 1; k &lt; slabs[0].getDimensions(); ++k) {<br />
      if(slabs[i].getDimension(k) &lt; slabs[j].getDimension(k)) {<br />
        res = false;<br />
        break;<br />
     }<br />
    }<br />
    if(res) {<br />
     if(max &lt; count[j]+1) {<br />
       max = count[j]+1;<br />
     }<br />
    }<br />
 }<br />
 count[i] = max;<br />
 }</p>
<p> int max = -1;<br />
 for(int i = 0; i &lt; slabs.length; ++i) {<br />
  if(max &lt; count[i])<br />
    max = count[i];<br />
}<br />
return max;<br />
}</p>
