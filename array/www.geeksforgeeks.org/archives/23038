<p>//Question 1<br />
int getMaxSet(Set[] sets) {<br />
 int[] count = new int[sets.length];<br />
 sortXDec(sets);</p>
<p> for(int i = 0; i &lt; sets.length; ++i) {<br />
  int max = -1;<br />
  //find index with X greater than current Y<br />
  int index = getIndex(sets[i].Y, sets);<br />
  int max = 0;<br />
  for(int j = 0; j &lt; index &amp;&amp; j &lt; i; ++j) {<br />
   if(max &lt; count[j]+1)<br />
    max = count[j]+1;<br />
  }<br />
  count[i] = max;<br />
 }</p>
<p> int max = -1;<br />
 for(int i = 0; i &lt;sets.length; ++i)<br />
  if(max &lt; count[i])<br />
   max = count[i];</p>
<p>return max;<br />
}</p>
<p>//Question 2<br />
int getLongestIncreasingSubs(int[] arr) {<br />
 int[] activeList = new int[arr.length];<br />
 int length;<br />
 activeList[0] = arr[0];</p>
<p> for(int i = 1; i &lt; arr.length. ++i) {<br />
	if(arr[i]  activeList[length-1])<br />
	 activeList[length++] = arr[i];<br />
	else {<br />
	 //Binary search operation<br />
	  int j = findIndex(activeList, arr[i], 0, length-1);<br />
	  ++j;<br />
	  activeList[j] = arr[i];<br />
	}<br />
}</p>
<p>return length;<br />
}</p>
<p>//Question 3<br />
int getSwappedLL(Node head) {<br />
 Node newHead = null;<br />
 Node currentNode = head;<br />
 Node tail = null;</p>
<p> while(currentNode != null) {<br />
  Node temp1 = currentNode;</p>
<p>  for(int i = 0; i = 0; &#8211;i) {<br />
  while(!s.empty()) {<br />
   int num = s.peek();<br />
   if(num &lt;= A[i])<br />
    s.pop();<br />
   else {<br />
     newArr[i] = num;<br />
     s.push(arr[i]);<br />
     break;<br />
    }<br />
  }<br />
  if(s.isEmpty())<br />
   newArr[i] = -1;<br />
 }</p>
<p>return newArr;<br />
}</p>
