 for(int i = 0; i &lt;sets.length; ++i)<br />
  if(max &lt; count[i])<br />
   max = count[i];</p>
<p>return max;<br />
}</p>
<p>//Question 2<br />
int getLongestIncreasingSubs(int[] arr) {<br />
 int[] activeList = new int[arr.length];<br />
 int length;<br />
 activeList[0] = arr[0];</p>
<p> for(int i = 1; i &lt; arr.length. ++i) {<br />
	if(arr[i]  activeList[length-1])<br />
	 activeList[length++] = arr[i];<br />
	else {<br />
	 //Binary search operation<br />
	  int j = findIndex(activeList, arr[i], 0, length-1);<br />
	  ++j;<br />
	  activeList[j] = arr[i];<br />
	}<br />
}</p>
<p>return length;<br />
}</p>
<p>//Question 3<br />
int getSwappedLL(Node head) {<br />
 Node newHead = null;<br />
 Node currentNode = head;<br />
 Node tail = null;</p>
<p> while(currentNode != null) {<br />
  Node temp1 = currentNode;</p>
<p>  for(int i = 0; i = 0; &#8211;i) {<br />
  while(!s.empty()) {<br />
   int num = s.peek();<br />
   if(num &lt;= A[i])<br />
    s.pop();<br />
   else {<br />
     newArr[i] = num;<br />
     s.push(arr[i]);<br />
     break;<br />
    }<br />
  }<br />
  if(s.isEmpty())<br />
   newArr[i] = -1;<br />
 }</p>
<p>return newArr;<br />
}</p>
<p>/* Paste your code here (You may delete these lines if not writing code) */</p>
</div>
        </div>

    </li><!-- #comment-## -->
    <li class="comment odd alt thread-odd thread-alt depth-1" id="dsq-comment-12839">
        <div id="dsq-comment-header-12839" class="dsq-comment-header">
            <cite id="dsq-cite-12839">
                <span id="dsq-author-user-12839">Srikant Aggarwal</span>
            </cite>
        </div>
        <div id="dsq-comment-body-12839" class="dsq-comment-body">
            <div id="dsq-comment-message-12839" class="dsq-comment-message"><p>//Interview Set 10</p>
<p>/********* Interview Round 1 ******************/</p>
<p>//Question 1<br />
Node getNewLinkedList(Node head, int K) {<br />
	Node firstNode = head;<br />
	Node secondNode = head;<br />
	int i = 1;<br />
	while(i &lt; K &amp;&amp; secondNode != null) {<br />
	  secondNode = secondNode.next;<br />
	}</p>
<p>	while(secondNode != null) {<br />
	 int temp = secondNode.data;<br />
	 secondNode.data = firstNode.data;<br />
	 firstNode.data = temp;</p>
<p>	 int i = 1;<br />
	 while(i &lt;= K &amp;&amp; secondNode != null) {<br />
	   secondNode = secondNode.next;<br />
	   firstNode = firstNode.next;<br />
	}<br />
	}</p>
<p>	return head;<br />
}</p>
<p>//Question 2<br />
Node getNewLinkedList(Node head, int K) {<br />
	Node firstNode = null;<br />
	Node secondNode = head;<br />
	int i = 1;<br />
	while(i &lt; K-1 &amp;&amp; secondNode != null) {<br />
	  secondNode = secondNode.next;<br />
	}</p>
<p>	while(secondNode != null) {<br />
	 if(firstNode == null) {<br />
	  Node temp = secondNode.next;<br />
	  secondNode.next = head;<br />
	  head = temp;<br />
	  firstNode = head;<br />
	  secondNode = secondNode.next;<br />
	  }<br />
	  else {<br />
	   Node temp = secondNode.next;<br />
	   secondNode.next = firstNode.next;<br />
	   firstNode.next = temp;<br />
	   firstNode = firstNode.next;<br />
	   secondNode = secondNode.next;<br />
	}</p>
<p>	 int i = 1;<br />
	 while(i &lt; K &amp;&amp; secondNode != null) {<br />
	   secondNode = secondNode.next;<br />
	   firstNode = firstNode.next;<br />
	}<br />
	}</p>
<p>	return head;<br />
}</p>
<p>/********* Interview Round 2 ******************/</p>
<p>//Question 1-3 KDimension</p>
<p>int getMaxSlab(Slab[] slabs) {<br />
  int[] count = new int[slabs.length];<br />
  max[0] = 1;<br />
  sortFirstDim(slabs);</p>
<p>  for(int i = 0; i &lt; slabs.length; ++i) {<br />
   int max = -1;<br />
   for(int j = 0; j &lt; i; ++j) {<br />
    boolean res = true;<br />
    for(int k = 1; k &lt; slabs[0].getDimensions(); ++k) {<br />
      if(slabs[i].getDimension(k) &lt; slabs[j].getDimension(k)) {<br />
        res = false;<br />
        break;<br />
     }<br />
    }<br />
    if(res) {<br />
     if(max &lt; count[j]+1) {<br />
       max = count[j]+1;<br />
     }<br />
    }<br />
 }<br />
 count[i] = max;<br />
 }</p>
<p> int max = -1;<br />
 for(int i = 0; i &lt; slabs.length; ++i) {<br />
  if(max &lt; count[i])<br />
    max = count[i];<br />
}<br />
return max;<br />
}</p>
</div>
        </div>

