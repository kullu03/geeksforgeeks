public int addOne(int n){
        int y=1;
// Find the position of zero from last
        while( (n&amp;y) !=0 ){
            y&lt;&lt;=1;
        }
// Set the zero as one
        n |=y;
// Set the rest of one after first occurrence of zero
        n &amp;= y;        
        return n;
    }
    
    int main() {
        printf(&quot;%d&quot;,addOne(3));
        return 0;
    }
