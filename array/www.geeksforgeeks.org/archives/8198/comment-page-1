            <div id="dsq-comment-message-4329" class="dsq-comment-message"><pre> <code language="C">
int addone(short int x)
{
	int y = 1;
	if( 0 == (x &amp; 1) )
		 x |= 1;
	else
	{
		for( y = 0; y &lt; (sizeof(x) * 8 ) &amp;&amp; (x &amp; 1 &lt;&lt; y ) ; y++ )
		{
			x &amp;= ~( 1 &lt;&lt; y );
		}
		x |= ( 1 &lt;&lt; y );
	}		
	return x;
}

int main()
{	
	int a = 16;
	printf(&quot; Add One : %d&quot;, addone( a ));
}
</code> </pre>
