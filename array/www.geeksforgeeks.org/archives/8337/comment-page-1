<pre> <code language="C">

#include&lt;iostream&gt;
using namespace std;

int main(void)
{
  static char *arr = {&quot;Even&#92;&#48;Odd&quot;};
  int no;
  cout &lt;&lt; &quot;Enter a number: &quot;;
  cin &gt;&gt; no;
  no &amp;= 1;
  cout &lt;&lt; arr + ((no &lt;&lt; 2) + no);
 
  return 0;
}
