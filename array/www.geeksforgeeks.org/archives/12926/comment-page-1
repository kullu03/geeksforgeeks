<p>int isSubset(int a[],int b[],int sizeA,int sizeB)<br />
{<br />
    int j=0;<br />
    int i;<br />
    int count = 0;<br />
    if(sizeA &gt;=sizeB)<br />
    {<br />
    for(i=0;i&lt;sizeA;i++)<br />
    {<br />
    if(b[j]==a[i])<br />
    {<br />
    count++;<br />
    j++;<br />
    i=0;<br />
    }<br />
    if(j==sizeB)<br />
    break;<br />
    }<br />
    if(count == sizeB)<br />
    printf(&quot;b[] is subset of a[]n&quot;);<br />
    else<br />
    printf(&quot;b[] is not a subset of a[]n&quot;);<br />
    }</p>
</div>
        </div>

    </li><!-- #comment-## -->
