void reverse_in_chunks(node **head,int k)
{
  node *prev,*current,*next,*tmp;
  int count=0;

  tmp=prev=current=next=NULL;
  current = *head;
  next = current-&gt;next;

   if(k &lt; 2)
	   return ;
   while (next != NULL)
   {
	   current-&gt;next=prev;
	   prev = current;
	   current = next;
	   next = next-&gt;next;
	   count++;

	   if (count == k &amp;&amp; prev !=NULL)
		   *head=prev;

	   if (count % k == 0 )
	   {

		   if(tmp == NULL)
		   {
			   tmp=prev;
		   }
