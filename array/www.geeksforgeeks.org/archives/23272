<p>int first_list_excluding_second_list(struct node *froot,struct node* sroot,struct node* troot)<br />
{<br />
	struct node *fp=froot,*sp=sroot,*tp=troot;</p>
<p>	while(fp!=NULL &amp;&amp; sp!=NULL)<br />
	{<br />
		if(sp-&gt;data &gt; fp-&gt;data)<br />
		{<br />
			insert(tp,fp-&gt;data);<br />
			fp=fp-&gt;next;<br />
		}<br />
		else if(sp-&gt; data == fp-&gt;data)<br />
		{<br />
			fp=fp-&gt;next;<br />
