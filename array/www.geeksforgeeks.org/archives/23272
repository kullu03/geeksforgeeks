            <div id="dsq-comment-message-10635" class="dsq-comment-message"><pre> <code language="C">
#include &lt;stdio.h&gt;
#include &lt;stdlib.h&gt;

typedef struct node {
  int val ;
  struct node *next ;
}node ;

node* getNode (int val) {
  node* n = (node *) malloc (sizeof(node)) ;
  n-&gt;val = val ;
  n-&gt;next = NULL ;
  return n ;
}

node* getSortedList (node *head) {
  node *zero = NULL, *one = NULL, *second = NULL, *temp ;
  node *tzero = NULL, *tone = NULL , *tsecond = NULL ;
  while ( head ) {
    temp = head ;
    head = head-&gt;next ;
    temp-&gt;next = NULL ;

    switch (temp-&gt;val) {
      case 0 : if (!zero) {
                zero = temp ;
                tzero = temp ;
              } else {
                tzero-&gt;next =temp ;
                tzero = temp ;
              }
              break ;
      case 1 : if (!one) {
                one = temp ;
                tone = temp ;
              } else {
                tone-&gt;next = temp ;
                tone = temp ;
              }
              break ;
      case 2 :
              if (!second) {
                second = temp ;
                tsecond = temp ;
              } else {
                tsecond-&gt;next = temp ;
                tsecond = temp ;
              }
              break ;
    }
  }

  if ( one == NULL )
    one = second ;
  else tone-&gt;next = second ;

  if ( zero == NULL )
    zero = one ;
  else tzero-&gt;next = one ;
  head = zero ;
  return head ;
}

void printList (node *head) {
  printf ( &quot;\n&quot; ) ;
  while (head) {
    printf ( &quot;%d &quot;, head-&gt;val ) ;
    head = head-&gt;next ;
  }
}

int main () {
  node* list = getNode (2) ;
  list-&gt;next = getNode (1) ;
  list-&gt;next-&gt;next = getNode (0) ;
  list-&gt;next-&gt;next-&gt;next = getNode (1) ;
  list-&gt;next-&gt;next-&gt;next-&gt;next = getNode (0) ;
  list = getSortedList (list) ;
  printList (list) ;
  return 0 ;
}

</code> </pre>
</div>
        </div>

