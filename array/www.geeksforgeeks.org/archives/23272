    <li class="comment even thread-even depth-1" id="dsq-comment-10612">
        <div id="dsq-comment-header-10612" class="dsq-comment-header">
            <cite id="dsq-cite-10612">
                <span id="dsq-author-user-10612">shivi</span>
            </cite>
        </div>
        <div id="dsq-comment-body-10612" class="dsq-comment-body">
            <div id="dsq-comment-message-10612" class="dsq-comment-message"><p>//shivi testing environment<br />
#include<br />
using namespace std;<br />
class LinkList<br />
{<br />
	private:<br />
	struct Node<br />
	{<br />
		int item;<br />
		Node *next;<br />
		Node (int i,Node *x)<br />
		{<br />
			item=i;<br />
			next=x;<br />
		}<br />
	};<br />
	Node *start;</p>
<p>	public:<br />
	LinkList()<br />
	{start=NULL;}</p>
<p>	void Add(int k)<br />
	{<br />
		Node *p=new Node(k,start);<br />
		start=p;<br />
	}</p>
<p>	void Use(Node *x,int arr[])<br />
	{<br />
		for(int j=0;j&lt;=2;++j)<br />
		for(int i=0;iitem=j;<br />
					x=x-&gt;next;<br />
			}<br />
	}<br />
	void Sort()<br />
	{<br />
		int arr[3]={0};<br />
		Node *x=start;<br />
		while(x!=NULL)<br />
		{<br />
			if(x-&gt;item==0) arr[0]++;<br />
			else if(x-&gt;item==1) arr[1]++;<br />
			else if(x-&gt;item==2) arr[2]++;<br />
			x=x-&gt;next;<br />
		}<br />
		cout&lt;&lt;endl&lt;&lt;arr[0]&lt;&lt;arr[1]&lt;&lt;arr[2]&lt;&lt;endl;<br />
		x=start;int i;<br />
		Use(x,arr);<br />
	}</p>
<p>	void Print()<br />
	{<br />
		Node *x=start;<br />
		while(x!=NULL)<br />
		{<br />
			cout&lt;item&lt;next;<br />
		}<br />
	}</p>
<p>};<br />
int main()<br />
{<br />
	LinkList l;<br />
	l.Add(2);<br />
	l.Add(1);<br />
	l.Add(0);<br />
	l.Add(0);<br />
	l.Add(2);<br />
	l.Add(1);<br />
	l.Add(2);<br />
	l.Print();<br />
	cout&lt;&lt;endl;<br />
	l.Sort();<br />
	l.Print()	;<br />
}</p>
</div>
