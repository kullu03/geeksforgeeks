    <li class="comment even thread-even depth-1" id="dsq-comment-10593">
        <div id="dsq-comment-header-10593" class="dsq-comment-header">
            <cite id="dsq-cite-10593">
http://www.cppblogs.com/icanth                <span id="dsq-author-user-10593">ICANTH</span>
            </cite>
        </div>
        <div id="dsq-comment-body-10593" class="dsq-comment-body">
            <div id="dsq-comment-message-10593" class="dsq-comment-message"><p>2nd Online Test 3:</p>
<pre> <code language="CPP">
#include&lt;stdio.h&gt;

typedef struct node_targ
{
	int data;
	struct node_targ *left;
	struct node_targ *right;
} Node;

typedef Node* Tree;

static inline void swap_two_nodes(Node *&amp;node1, Node *&amp;node2)
{
	int tmp = node1-&gt;data;
	node1-&gt;data = node2-&gt;data;
	node2-&gt;data = tmp;
}

// case 1: A[1] &gt; A[2]
// case 2: A[N] &lt; A[N-1]
// case 3:A[i-1] &gt; A[i] &lt; A[i+1]
// case 4:A[i-1] &lt; A[i] &gt; A[i+1]
static bool _adjust_two_nodes(Node *node, Node *&amp;prev, Node *&amp;prev_prev, Node *&amp;first)
{
	if(node == NULL)	return false;
	else if(_adjust_two_nodes(node-&gt;left, prev, prev_prev, first))
	{
		return true;
	}
	else if(prev != NULL &amp;&amp;
		(prev_prev == NULL &amp;&amp; prev-&gt;data &gt; node-&gt;data)
		|| (prev_prev != NULL &amp;&amp; prev_prev-&gt;data &gt; prev-&gt;data &amp;&amp; prev-&gt;data &lt; node-&gt;data)
		|| (prev_prev != NULL &amp;&amp; prev_prev-&gt;data &lt; prev-&gt;data &amp;&amp; prev-&gt;data &gt; node-&gt;data))
	{ // when case1: case3: case4:
		if(first == NULL)
		{
			first = prev;
		}
		else
		{
			swap_two_nodes(first, prev);
			return true;
		}
	} // if
	prev_prev = prev;
	prev = node;
	return _adjust_two_nodes(node-&gt;right, prev, prev_prev, first);
} // adjust_two_nodes

bool adjust_two_nodes(Tree &amp;tree)
{
	if(tree == NULL)	return false;

	Node *prev = NULL, *prev_prev = NULL, *first = NULL;
	if(_adjust_two_nodes(tree, prev, prev_prev, first))
	{
		return true;
	}
	else if(first != NULL)
	{ // when case 2:
		swap_two_nodes(first, prev);
		return true;
	}
	else
	{
		return false;
	}
} // adjust_two_nodes
</code> </pre>
</div>
        </div>

