    <li class="comment even thread-odd thread-alt depth-1" id="dsq-comment-9520">
        <div id="dsq-comment-header-9520" class="dsq-comment-header">
            <cite id="dsq-cite-9520">
                <span id="dsq-author-user-9520">lohith</span>
            </cite>
        </div>
        <div id="dsq-comment-body-9520" class="dsq-comment-body">
            <div id="dsq-comment-message-9520" class="dsq-comment-message"><pre> <code language="Java">

public class MinimumJumps2 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
     
		int arr[] = {1, 3, 5, 8, 9, 2, 6, 7, 6, 8, 9};
		
		int ans =minumumJumps(arr,arr.length-1,arr[arr.length-1]);
		System.out.println(ans);
	}

	private static int minumumJumps(int[] arr, int target, int stepsBackward) {
		
		if(target == 0){
			return 0;
		}
		
		int min = (target-stepsBackward) +1 &gt; 0 ? (target-stepsBackward) +1  : 0 ;
		
		int minimum = Integer.MAX_VALUE;
		
		for(int i=min;i&lt;target;i++){
			if(arr[i]+i &gt;= target ){
				int temp = minumumJumps(arr,i,arr[i]);
				if(temp &lt; minimum){
					minimum = temp;
					break;
				}
					
			}
		}
		return minimum+1;
	}

	
}

