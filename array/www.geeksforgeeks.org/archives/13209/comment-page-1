    <li class="comment even thread-odd thread-alt depth-1" id="dsq-comment-7606">
        <div id="dsq-comment-header-7606" class="dsq-comment-header">
            <cite id="dsq-cite-7606">
                <span id="dsq-author-user-7606">kartikaditya</span>
            </cite>
        </div>
        <div id="dsq-comment-body-7606" class="dsq-comment-body">
            <div id="dsq-comment-message-7606" class="dsq-comment-message"><p>public class MinJumps {<br />
    public static int getMinJumps(int a[]) {<br />
        if (a == null || a.length == 0) {<br />
            return -1;<br />
        }<br />
        for (int i = 0; i &lt; a.length; ++i) {<br />
            if (a[i] &lt; 0) {<br />
                return -1;<br />
            }<br />
        }</p>
<p>        int dp[] = new int[a.length];<br />
        dp[a.length &#8211; 1] = 0;<br />
        for (int i = a.length &#8211; 2; i &gt;= 0; &#8211;i) {<br />
            int min = Integer.MAX_VALUE;<br />
            for (int j = i + 1; j &lt; a.length &amp;&amp; j &lt;= i + a[i]; ++j) {<br />
                if (dp[j] != Integer.MAX_VALUE &amp;&amp; 1 + dp[j] &lt; min) {<br />
                    min = 1 + dp[j];<br />
                }<br />
            }<br />
            dp[i] = min;<br />
        }<br />
        System.out.println(dp[0]);<br />
        return dp[0];<br />
    }</p>
<p>    public static void main(String args[]) {<br />
        getMinJumps(new int[]{1, 3, 5, 8, 9, 2, 6, 7, 6, 8, 9});<br />
        getMinJumps(new int[]{1, 3, 6, 1, 0, 9});<br />
        getMinJumps(new int[]{1, 0, 1, 2});<br />
    }<br />
}</p>
