<pre> <code language="C">

for(int i=0;i&lt;n/2;i++){
    for(int j=0;j&lt;n+1/2;j++){
       exchange(a[i][j],a[n-j-1][i],a[n-i-1][n-j-1],a[j][n-1-i]);
    }
}

exchange(int &amp;a, int &amp;b, int &amp;c, int &amp;d){
      int temp;
      temp = a;
      a=b;
      b=c;
      c=d;
      d=temp;
}

</code> </pre>
