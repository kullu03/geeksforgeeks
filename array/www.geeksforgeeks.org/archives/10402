            <div id="dsq-comment-message-21271" class="dsq-comment-message"><pre> <code language="C">
//shivi..coding is adictive!!
#include&lt;shiviheaders.h&gt;
#define M 4
#define N 7
using namespace std;
int main()
{
	char brr[N][M];
	
	char arr[][N]=
	{
	{'*','*','*','^','*','*','*'},{'*','*','*','|','*','*','*'},{'*','*','*','|','*','*','*'},{'*','*','*','|','*','*','*'}
	};
	
	int x=N-1,y=M-1;
	
	for(int i=0;i&lt;M;++i)
	{
		for(int j=0;j&lt;N;++j)
		{
			if(arr[i][j]=='|')brr[x--][y]='-';
			if(arr[i][j]=='^')brr[x--][y]='&gt;';
			if(arr[i][j]=='*')brr[x--][y]=arr[i][j];
			
		}
		y--;
		x=N-1;
	}
	for(int i=0;i&lt;M;++i)
	{
		for(int j=0;j&lt;N;++j)
		cout&lt;&lt;arr[i][j];
		cout&lt;&lt;endl;
	}
	cout&lt;&lt;endl&lt;&lt;endl;
	for(int i=0;i&lt;N;++i)
	{
		for(int j=0;j&lt;M;++j)
		cout&lt;&lt;brr[i][j];
		cout&lt;&lt;endl;
	}
	
}
</code> </pre>
