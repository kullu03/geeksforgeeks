<p>for(int i=0; i &lt; n / 2 ; i++)<br />
  for(int j=0; j&lt;(n+1)/2; j++)<br />
    cyclic_roll(m[i][j], m[n-1-j][i], m[n-1-i][n-1-j], m[j][n-1-i]);</p>
<p>void cyclic_roll(int &amp;a, int &amp;b, int &amp;c, int &amp;d)<br />
{<br />
   int temp = a;<br />
   a = b;<br />
   b = c;<br />
   c = d;<br />
   d = temp;<br />
} </p>
<p></code></p>
