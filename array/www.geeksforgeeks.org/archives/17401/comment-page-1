    <li class="comment odd alt thread-odd thread-alt depth-1" id="dsq-comment-9522">
        <div id="dsq-comment-header-9522" class="dsq-comment-header">
            <cite id="dsq-cite-9522">
                <span id="dsq-author-user-9522">lohith</span>
            </cite>
        </div>
        <div id="dsq-comment-body-9522" class="dsq-comment-body">
            <div id="dsq-comment-message-9522" class="dsq-comment-message"><pre> <code language="Java">
import java.util.ArrayList;
import java.util.HashMap;

public class Coins {

	/**
	 * @param args
	 */
	
	
	
	
	public static void main(String[] args) {
		int N = 4;
		int[] S = { 1, 2, 3 };

		HashMap&lt;Integer, Integer&gt; map = new HashMap&lt;Integer, Integer&gt;();
		for(int i=0;i&lt;S.length;i++){
			map.put(S[i], 0);
		}
		ArrayList&lt;HashMap&lt;Integer, Integer&gt;&gt; values = calcualte(N, S,map);

		for (HashMap&lt;Integer, Integer&gt; temp : values)
			System.out.println(temp);

	}

	private static ArrayList&lt;HashMap&lt;Integer, Integer&gt;&gt; list = new ArrayList&lt;HashMap&lt;Integer, Integer&gt;&gt;();

	private static ArrayList&lt;HashMap&lt;Integer, Integer&gt;&gt; calcualte(int n,
			int[] s, HashMap&lt;Integer, Integer&gt; map) {

		if (n == 0) {
			list.add(map);
			return list;
		}

		for (int i = 0; i &lt; s.length; i++) {
			if (n - s[i] &gt;= 0) {
				
			     HashMap&lt;Integer, Integer&gt; temp = (HashMap&lt;Integer, Integer&gt;)map.clone();
				 temp.put( s[i], temp.get(s[i])+1);
				 calcualte(n- s[i], s,temp);
				
			}

		}

		return list;
	}

}






</code> </pre>
</div>
        </div>

