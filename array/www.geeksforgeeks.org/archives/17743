            <div id="dsq-comment-message-22533" class="dsq-comment-message"><p>Just this:</p>
<pre> <code language="C">
void _printString(char *x,int m,char *y,int n,int i,int j,char *res,int index)
{
    if(index==(m+n)){res[index]='&#092;&#048;';printf(&quot;%s\n&quot;,res);}
    else
    {
        if(i&lt;m) {res[index]=x[i]; _printString(x,m,y,n,i+1,j,res,index+1);}
        if(j&lt;n) {res[index]=y[j]; _printString(x,m,y,n,i,j+1,res,index+1);}
    }
}

void printString(char *x,int m,char *y,int n)
{
    char *str=(char*)malloc((m+n)*sizeof(char));
    _printString(x,m,y,n,0,0,str,0);
}

int main()
{
    char str1[]=&quot;ABC&quot;;
    char str2[]=&quot;DE&quot;;
    printString(str1,3,str2,2);
    return 0;
}
</code> </pre>
