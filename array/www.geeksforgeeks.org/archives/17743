            <div id="dsq-comment-message-7548" class="dsq-comment-message"><pre> <code language="C">
#include&lt;stdio.h&gt;
#include&lt;string.h&gt;

void printallinterleaving(char *a,char *b,char *c,int alen,int blen,int i)
{
	if(!*a &amp;&amp; !*b)
	{
		c[i]='&#92;&#48;';
		printf(&quot;%s\n&quot;,c);
		return ;
	}
	if(*a)
	{
		c[i]=*a;
		printallinterleaving(a+1,b,c,alen-1,blen,i+1);
		
	}
	if(*b)
	{
		c[i]=*b;
		printallinterleaving(a,b+1,c,alen,blen-1,i+1);
		
	}
}

int main()
{
	char a[10],b[10],c[20];
	scanf(&quot;%s%s&quot;,a,b);	
	printallinterleaving(a,b,c,strlen(a),strlen(b),0);
return 0;
}
