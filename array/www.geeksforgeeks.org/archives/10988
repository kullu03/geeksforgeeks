public boolean ancestor(Node root, int value) {
         
         if (root == null)
             return false;
         if (root.key == value)
             return true;
         if(ancestor(root.leftChild, value) || ancestor(root.rightChild, value)) {
             System.out.println(&quot;Value is: &quot;+ root.key);
             return true;
         }
         else
             return false;
         
         
