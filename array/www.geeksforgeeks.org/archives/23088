    <li class="comment even thread-odd thread-alt depth-1" id="dsq-comment-10451">
        <div id="dsq-comment-header-10451" class="dsq-comment-header">
            <cite id="dsq-cite-10451">
                <span id="dsq-author-user-10451">Vinay Khetan</span>
            </cite>
        </div>
        <div id="dsq-comment-body-10451" class="dsq-comment-body">
            <div id="dsq-comment-message-10451" class="dsq-comment-message"><p>//Round2 question 4<br />
import java.util.*;</p>
<p>class mystack{<br />
Stack&lt;Integer&gt; orig;<br />
Stack&lt;Integer&gt; min ;<br />
//int size;</p>
<p>mystack()<br />
	{<br />
	orig=new Stack&lt;Integer&gt;();<br />
	min=new Stack&lt;Integer&gt;();<br />
	}</p>
<p>public void mypush(int x)<br />
	{<br />
	Integer y=new Integer(x);<br />
	orig.push(y);<br />
	if(min.empty() || y &lt;min.peek() || y==min.peek())<br />
		min.push(y);<br />
	}</p>
<p>public Integer mypop()<br />
	{<br />
	Integer temp=orig.pop();<br />
	if(temp==min.peek())<br />
		min.pop();<br />
	return temp;<br />
	}</p>
<p>public Integer minimum()<br />
	{<br />
	return min.peek();<br />
	}</p>
<p>public int getsize()<br />
	{<br />
	return orig.size();<br />
	}<br />
}</p>
<p>class Main<br />
	{<br />
	public static void main(String args[])<br />
	{<br />
	mystack st=new mystack();<br />
	st.mypush(4);<br />
	st.mypush(5);<br />
	st.mypush(3);<br />
	st.mypush(7);<br />
	st.mypush(9);<br />
	st.mypush(2);<br />
	st.mypush(1);<br />
	System.out.println(&quot;Minimum number is :&quot; + st.minimum());<br />
	st.mypop();<br />
	System.out.println(&quot;Minimum number is :&quot; + st.minimum());<br />
	st.mypop();<br />
	System.out.println(&quot;Minimum number is :&quot; + st.minimum());<br />
	st.mypop();<br />
	System.out.println(&quot;Minimum number is :&quot; + st.minimum());<br />
	}<br />
}</p>
</div>
        </div>

