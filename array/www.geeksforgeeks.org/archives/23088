        </div>
        <div id="dsq-comment-body-29507" class="dsq-comment-body">
            <div id="dsq-comment-message-29507" class="dsq-comment-message"><p>//Question 1: Write a modified strcmp function which ignores cases and //returns -1 if s1 &lt; s2, 0 if s1 = s2, else returns 1</p>
<p>#include<br />
int stringCmp(char *s1,char *s2)<br />
{<br />
    while((*s1==*s2)||(*s1==*s2+32)||(*s1+32==*s2))<br />
    {<br />
        if(*s1==&#8221;)<br />
            return 0;<br />
            s1++;<br />
            s2++;<br />
    }<br />
    if(*s1-*s2&gt;0)<br />
        return 1;<br />
    else<br />
        return -1;<br />
}<br />
int main()<br />
{<br />
     char s1[100],s2[100];<br />
     printf(&#8220;Enter the string1n&#8221;);<br />
     gets(s1);<br />
     printf(&#8220;Enter the String2n&#8221;);<br />
     gets(s2);<br />
     printf(&#8220;%d&#8221;,stringCmp(s1,s2));<br />
     return 0;<br />
}</p>
</div>
        </div>

    </li><!-- #comment-## -->
    <li class="comment odd alt thread-odd thread-alt depth-1" id="dsq-comment-14617">
        <div id="dsq-comment-header-14617" class="dsq-comment-header">
            <cite id="dsq-cite-14617">
                <span id="dsq-author-user-14617">Aishwarya</span>
            </cite>
        </div>
        <div id="dsq-comment-body-14617" class="dsq-comment-body">
            <div id="dsq-comment-message-14617" class="dsq-comment-message"><pre> <code language="C">
// just put a file &quot;file.txt&quot; with some text in it
#include&lt;iostream&gt;
#include&lt;fstream&gt;
#include&lt;vector&gt;
#include&lt;map&gt;
#define N 10
using namespace std;
void toLow(char *s)
{
   while(*s)
   {
      //if(*s&gt;=65 &amp;&amp; *s&lt;=90)*s+=32;
   }
}
int main(void )
{
   string s, _s;
   char *file=&quot;file.txt&quot;;
   char *token=NULL, *delimiters=&quot; \t\n,*&gt;&lt;.&quot;;
   typedef map&lt;string,int&gt; myMap;
   typedef map&lt;int,vector&lt;string&gt; &gt; resultMap;
   myMap m;
   myMap::iterator im;
   resultMap r;
   resultMap::reverse_iterator ir;
   vector&lt;int&gt; v;
   vector&lt;string&gt;::iterator it;
   int limit;
   ifstream f(file);
   // read all words in file and store them in a map
   if(f.is_open())
   {
      while(!f.eof())
      {
         getline(f,s);
         /* c_str return a const char*, and so it needs to be typecasted */
         token=strtok((char*)s.c_str(),delimiters);
         while(token)
         {
            m[token]++;
            token=strtok(NULL,delimiters);
         }
      }
   }
   else
   {
      cout&lt;&lt;&quot;cannot open file\n&quot;;
      return 1;
   }
   // find top N frequencies
   for(im=m.begin();im!=m.end();im++)
      if(find(v.begin(),v.end(),im-&gt;second)==v.end())v.push_back(im-&gt;second);
   sort(v.begin(),v.end());
   // find the threshold
   if(v.size()&gt;=N-1)limit=v[v.size()-N];
   else limit=v[0];
   // get all words with count &gt;= threshold
   for(im=m.begin();im!=m.end();im++)
      if(im-&gt;second&gt;=limit)r[im-&gt;second].push_back(im-&gt;first);
   // show the result
   for(ir=r.rbegin();ir!=r.rend();ir++)
   {
      printf(&quot;%4d  times\n&quot;,ir-&gt;first);
      cout&lt;&lt;&quot;-----------\n&quot;;
      for(it=ir-&gt;second.begin();it!=ir-&gt;second.end();it++)
         cout&lt;&lt;*it&lt;&lt;&quot; &quot;;
      cout&lt;&lt;&quot;\n\n&quot;;
   }
   getchar();
   return 0;
}
</code> </pre>
</div>
