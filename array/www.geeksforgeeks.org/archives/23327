<p>void find_result ( int32_t array[] , int32_t sum , int32_t count , int32_t number ) {<br />
        if (number&gt;=max || sum&lt;0 )<br />
                return;<br />
        if (sum==0 &amp;&amp; count==4) {<br />
                print ( count );<br />
                count&#8211;;<br />
                sum=sum-array[number];<br />
        }<br />
        find_result ( array , sum , count , number+1 );<br />
        temp_array[count++]=array[number];<br />
        sum=sum-array[number];<br />
        find_result ( array , sum , count , number+1 );<br />
}</p>
