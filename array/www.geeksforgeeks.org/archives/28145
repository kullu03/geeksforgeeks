            <div id="dsq-comment-message-13319" class="dsq-comment-message"><pre> <code language="C">
#include&lt;stdio.h&gt;


int find_missing_element(int arr[],int size)
{
	/*check_sum uses the consecutive numbers property*/
	int check_sum = size+2;

	if(arr[0] != 1)
		return 1;

	/* This condition checks if there is any missing number or not*/
	else if(arr[size-1] != size+1)
		return -1;

	for(int iter = 0;iter &lt;=size/2 ;++iter)
	{
		if(arr[iter] + arr[size-iter-1] == check_sum )
			continue ;
		else
		{
			if(arr[iter] - arr[iter-1] != 1)
				return arr[iter]-1;
			else
				return arr[size-iter-1] +1;
		}
	}

}

int main()
{
	int arr[] = {1,2,3,4,5,7,8};
	int size = sizeof(arr)/sizeof(arr[0]);

	int missing_num = find_missing_element(arr,size);
	if(missing_num != -1)
		printf(&quot;The missing number is : %d\n&quot;,missing_num);
	else
		printf(&quot;No number is missing\n&quot;);

}
</code> </pre>
