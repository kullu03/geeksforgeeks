            <div id="dsq-comment-message-13200" class="dsq-comment-message"><p>#include<br />
using namespace std;<br />
#include<br />
int main()<br />
{</p>
<p>	int n;<br />
	cout&lt;&gt;n;</p>
<p>	int *a;<br />
	a=(int *)malloc(n*sizeof(int));<br />
	int i=0;</p>
<p>	cout&lt;&lt;&quot;=\n Enter the number b/w 1 &amp; &quot;&lt;&lt;n+1&lt;&lt;&quot; in which only one will two times reapeate::&quot;;<br />
	int sum=0;<br />
	for(i=0;i&lt;n;i++)<br />
	{<br />
		cout&lt;&lt;&quot;\n Enter the value a[&quot;&lt;&lt;i&lt;&gt;a[i];<br />
		sum+=a[i];<br />
	}</p>
<p>	int miss;<br />
	for(int i=0;i&lt;n;i++)<br />
	{<br />
		if((sum-a[i]+1)==((n*(n+1))/2))<br />
		{<br />
			miss=a[i];<br />
			break;<br />
		}<br />
	}</p>
<p>	cout&lt;&lt;&quot;\n The missing number is :: &quot;&lt;&lt;miss;<br />
	return 0;<br />
}</p>
</div>
        </div>

