    <li class="comment odd alt thread-even depth-1" id="dsq-comment-10056">
        <div id="dsq-comment-header-10056" class="dsq-comment-header">
            <cite id="dsq-cite-10056">
                <span id="dsq-author-user-10056">Sreenivas Doosa</span>
            </cite>
        </div>
        <div id="dsq-comment-body-10056" class="dsq-comment-body">
            <div id="dsq-comment-message-10056" class="dsq-comment-message"><p>Please see the below code. Please let me know if anything goes wrong</p>
<pre> <code language="C">

bool Graph::isCyclic() {

	/*
	 * If there exists atleast one cycle return true
	 */
	bool* visited = new bool[V];

	for(int i = 0; i &lt; V; i++) {

		for(int j = 0; j &lt; V; j++) {
			visited[j] = false;
		}

		if(isCyclicUtil(i, i, visited)) {
			return true;
		}
	}
	return false;
}


bool Graph::isCyclicUtil(int node, int origin, bool visited[]) {

	if(!visited[node]) {

		visited[node] = true;

		list&lt;int&gt;::iterator itr;
		for(itr = adj[node].begin(); itr != adj[node].end(); ++itr) {

			if(isCyclicUtil(*itr, origin, visited)) {
				return true;
			}
		}

	} else {
		// already visited
		if(node == origin) {
			return true;
		}
	}
	return false;
}
</code> </pre>
</div>
        </div>

